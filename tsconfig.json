{
  "compilerOptions": {
    "target": "ES6",
    "esModuleInterop": true,
    "module": "nodenext",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
